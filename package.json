{
  "name": "@refinist/eslint-config",
  "version": "1.0.4",
  "description": "ESLint config for @refinist.",
  "keywords": [
    "eslint",
    "eslint-config",
    "flat",
    "eslint-flat-config",
    "refinist"
  ],
  "homepage": "https://github.com/refinist/eslint-config#readme",
  "bugs": {
    "url": "https://github.com/refinist/eslint-config/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/refinist/eslint-config.git"
  },
  "license": "MIT",
  "author": "Jeff Wang",
  "type": "module",
  "exports": {
    ".": "./dist/index.js",
    "./react": "./dist/react.js",
    "./package.json": "./package.json"
  },
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsdown",
    "dev": "eslint",
    "format": "prettier --write .",
    "format-check": "prettier --check .",
    "lint": "eslint",
    "lint:fix": "pnpm run lint --fix",
    "prepublishOnly": "pnpm run build",
    "release": "bumpp && pnpm publish",
    "typecheck": "tsc --noEmit"
  },
  "prettier": "@refinist/prettier-config",
  "dependencies": {
    "@eslint/js": "^9.28.0",
    "@stylistic/eslint-plugin": "^4.4.1",
    "eslint-config-flat-gitignore": "^2.1.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-prettier": "^5.4.1",
    "eslint-plugin-vue": "^10.1.0",
    "globals": "^16.2.0",
    "local-pkg": "^1.1.1",
    "prettier": "^3.5.3",
    "typescript-eslint": "^8.33.0",
    "vue-eslint-parser": "^10.1.3"
  },
  "devDependencies": {
    "@refinist/prettier-config": "^0.0.3",
    "@types/node": "^22.15.29",
    "bumpp": "^10.1.1",
    "eslint": "^9.28.0",
    "tsdown": "^0.13.0",
    "typescript": "^5.8.3"
  },
  "peerDependencies": {
    "@eslint-react/eslint-plugin": "^1.52.3",
    "eslint": "^9.28.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20"
  },
  "peerDependenciesMeta": {
    "@eslint-react/eslint-plugin": {
      "optional": true
    },
    "eslint-plugin-react-hooks": {
      "optional": true
    },
    "eslint-plugin-react-refresh": {
      "optional": true
    }
  },
  "packageManager": "pnpm@10.13.1",
  "engines": {
    "node": ">=20.0.0"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  }
}
